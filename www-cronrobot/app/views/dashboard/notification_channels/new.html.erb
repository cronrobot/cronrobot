
<%= form_with model: [:dashboard, @notification_channel] do |form| %>
  <%= form.hidden_field :project_id %>

  <div class="form-group mb-2">
    <%= form.label :name, "Name" %>
    <%= form.text_field :name, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :type, "Type" %>
    <%= form.select :type, NotificationChannel::NOTIFICATION_CHANNEL_TYPES, {}, class: "form-control" %>
  </div>

  <div class="form-group">
    
  </div>

  <%= form.submit %>
<% end %>
