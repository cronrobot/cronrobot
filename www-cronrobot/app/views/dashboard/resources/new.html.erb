
<%= form_with model: [:dashboard, @resource] do |form| %>
  <%= form.hidden_field :reference_id %>

  <div class="form-group mb-2">
    <%= form.label "params[name]", "Name" %>
    <%= form.text_field "params[name]", class: "form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :sub_type, "Type" %>
    <%= form.select :sub_type, ResourceProject::SUB_TYPES, {}, class: "form-control" %>
  </div>

  <div class="form-group">
    
  </div>

  <%= form.submit %>
<% end %>
